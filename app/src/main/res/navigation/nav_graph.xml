<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/drawFragment">

    <fragment
        android:id="@+id/drawFragment"
        android:name="pl.gda.wsb.drawapp.ui.draw.DrawFragment"
        android:label="DrawFragment" >
        <action
            android:id="@+id/navigateToRanking"
            app:destination="@id/rankingFragment" />
    </fragment>
    <fragment
        android:id="@+id/rankingFragment"
        android:name="pl.gda.wsb.drawapp.ui.ranking.RankingFragment"
        android:label="RankingFragment">
        <action
            android:id="@+id/navigateToDraw"
            app:destination="@id/drawFragment" />
    </fragment>
</navigation>